      $(this).parent().toggleClass('open');
    });
  });
  $('#nav-expander').on('click',function(e){
  	e.preventDefault();
  	$('.sidebar-nav').toggleClass('nav-expanded');
  	$('.nav-expander').toggleClass('nav-expander-expanded');
  	if($('.sidebar-nav').hasClass('nav-expanded')) {
